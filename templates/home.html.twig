{% extends 'base.html.twig' %}
{% block title %}
    Accueil
{% endblock %}
{% block
content %}
    <main>
       
        <div id="articles-page">
            <section>
                <h2>
                    Nos annonces de maison
                </h2>
                <div id="house-container">
                    {% for product in productsHouses %}
                        {% include '_partials/_card.html.twig' %}
                    {% endfor %}
                </div>
            </section>
            <section>
                <h2>
                    Nos annonces d'appartement
                </h2>
                <div id="apartment-container">
                    {% for product in productsApartments %}
                        {% include '_partials/_card.html.twig' %}
                    {% endfor %}
                </div>
            </section>
        </div>
    </main>
{% endblock %}
